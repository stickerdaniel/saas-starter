		"bits-ui": "^2.14.1",
		"clsx": "^2.1.1",
		"convex": "^1.30.0",
		"convex-svelte": "^0.0.11",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"dotenv": "^16.6.1",
