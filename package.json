{
	"name": "saas-starter",
	"version": "0.1.0",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev:frontend": "vite dev --open",
		"dev:backend": "bunx convex dev",
		"dev": "bun run dev:frontend & bun run dev:backend",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "husky",
		"package": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint .",
		"test:e2e": "playwright test",
		"test": "bun run test:e2e && bun run test:unit",
		"test:unit": "vitest --run --reporter=verbose --passWithNoTests",
		"i18n:pull": "bunx dotenv -f .env.local run -- bunx tolgee pull",
		"i18n:push": "bunx dotenv -f .env.local run -- bunx tolgee push",
		"i18n:cleanup": "bunx dotenv -f .env.local run -- bunx tolgee tag --filter-not-extracted --filter-tag production --tag deprecated --untag production",
		"setup:test-user": "bunx tsx scripts/setup-test-user.ts"
	},
	"dependencies": {
		"@convex-dev/agent": "^0.3.0",
		"@convex-dev/better-auth": "^0.9.11",
		"@convex-dev/resend": "^0.1.13",
		"@mmailaender/convex-better-auth-svelte": "^0.4.0",
		"@openrouter/ai-sdk-provider": "^1.2.3",
		"@oslojs/crypto": "^1.0.1",
		"@rive-app/canvas": "^2.32.0",
		"@stickerdaniel/convex-autumn-svelte": "^0.1.3",
		"@svelte-put/lockscroll": "^2.0.1",
		"@tolgee/cli": "^2.14.0",
		"@tolgee/svelte": "^6.2.7",
		"@zumer/snapdom": "^1.9.14",
		"ai": "^5.0.93",
		"atmn": "^0.0.28",
		"autumn-js": "^0.1.43",
		"better-auth": "1.3.34",
		"isomorphic-dompurify": "^2.32.0",
		"konva": "^10.0.8",
		"marked": "^17.0.0",
		"mode-watcher": "^1.1.0",
		"motion-start": "^0.1.18",
		"motion-sv": "^0.1.2",
		"posthog-js": "^1.290.0",
		"resend": "^6.5.2",
		"runed": "^0.36.0",
		"svelte-konva": "^1.0.1",
		"svelte-motion": "^0.12.2",
		"svelte-streamdown": "^2.6.1",
		"vercel": "^47.1.4"
	},
